    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/jquery-transit/jquery.transit.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/bootstrap/assets/javascripts/bootstrap/dropdown.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/bootstrap/assets/javascripts/bootstrap/collapse.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/bootstrap-fileinput/js/fileinput.min.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/autosize/jquery.autosize.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/isotope/dist/isotope.pkgd.min.js"></script>
    <script type="text/javascript" src="http://www.casavue.com/assets/libraries/OwlCarousel/owl-carousel/owl.carousel.min.js"></script>
